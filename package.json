{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "anki-add-english-word",
  "title": "Anki add English word",
  "description": "Add English word to Anki with a sentence that includes the word.",
  "icon": "command-icon.png",
  "author": "Yoke",
  "categories": [
    "Applications"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "add_english_word",
      "title": "Anki add English word",
      "description": "Input a word and a sentence that includes the word.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@iamtraction/google-translate": "^2.0.1",
    "@raycast/api": "^1.70.2",
    "@raycast/utils": "^1.4.16",
    "axios": "^1.4.0",
    "cheerio": "^1.0.0-rc.12"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/babel__core": "^7.20.5",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
